{
  "name": "csv-debt-calculator",
  "version": "1.0.0",
  "description": "Simple debt calculator.",
  "scripts": {
    "start": "node ./src/index.js",
    "debug": "node --inspect-brk ./src/index.js",
    "test": "nyc --reporter=html --reporter=text --reporter=json mocha --require ./tests/testSetup.js -- ./tests/**/*.test.js",
    "test:watch": "mocha -w --require ./tests/testSetup.js -- ./tests/**/*.test.js",
    "test:debug": "mocha --inspect-brk --require ./tests/testSetup.js -- ./tests/**/*.test.js",
    "build": "gulp build",
    "lint": "eslint --fix ./src ./tests"
  },
  "keywords": [
    "node",
    "mocha",
    "chai",
    "mockery",
    "nyc",
    "coverage",
    "TDD",
    "BDD"
  ],
  "author": "Adam Faryna <adamfaryna@appdy.net>",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-chmod": "^2.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-copy": "^1.0.1",
    "gulp-header": "^1.8.9",
    "mocha": "^4.0.1",
    "mock-require": "^2.0.2",
    "mockery": "^2.1.0",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2",
    "webpack": "^3.8.1",
    "webpack-stream": "^4.0.0"
  }
}
